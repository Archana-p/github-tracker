
		
<div class="container">
  <%= link_to "<span class='btn-label'><i class='glyphicon glyphicon-chevron-left'></i></span>Back".html_safe ,repos_path(),class:"btn btn-labeled btn-default" %>  
	<div class="row">
		 	
		<div class="widget-header ">
		<div class="col-md-12">
      <h4><%= @repo.github_repo_name.humanize%> </h4><span><%= link_to "Github<span class='btn-label'><i class='glyphicon glyphicon-new-window'></i></span>".html_safe,@repo.github_repo%></span>


        <div class="table-responsive">
            <table id="mytable" class="table table-bordred table-striped">
              <thead>
								<tr>
									<th><input type="checkbox" id="checkall" /></th>
									<th>Committer</th>
									<th>Commit</th>
									<th>Message</th>
									<th>Commit Date</th>
									
                  <th>Delete</th>
									
								</tr>
							</thead>
							<% @notification.each do |notification| %>
								<tbody>
									<tr>
										<td><input type="checkbox" class="checkthis" /></td>
										<td><%= notification.commiter_name%></td>
										<td><%=notification.commit%></td>
										<td><%= notification.commit_message.humanize%></td>
										<td><%= notification.commit_date%></td>
										
    								<td>
    									<p><button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" ><span class="glyphicon glyphicon-trash"></span></button></p>
    								</td>
									</tr>
								</tbody>
							<%end%>
    				</table>
        </div>
            
    </div>
</div>
<%= render :partial => "/repos/delete_modal"%>

    
   