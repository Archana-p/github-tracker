
		
<div class="container">
	<div class="row">
		 <%= link_to "<span class='btn-label'><i class='glyphicon glyphicon-chevron-left'></i></span>Back".html_safe ,repos_path(),class:"btn btn-labeled btn-default" %>		
		<div class="widget-header ">
		<div class="col-md-12">
      <h4><%= @repo.github_repo_name%></h4>
        <div class="table-responsive">
            <table id="mytable" class="table table-bordred table-striped">
              <thead>
								<tr>
									<th><input type="checkbox" id="checkall" /></th>
									<th>Committer</th>
									<th>Commit</th>
									<th>Message</th>
									<th>Commit Date</th>
									
                  <th>Delete</th>
									
								</tr>
							</thead>
							<% @notification.each do |notification| %>
								<tbody>
									<tr>
										<td><input type="checkbox" class="checkthis" /></td>
										<td><%= notification.commiter_name%></td>
										<td><%=notification.commit%></td>
										<td><%= notification.commit_message.humanize%></td>
										<td><%= notification.commit_date%></td>
										
    								<td>
    									<p><button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" ><span class="glyphicon glyphicon-trash"></span></button></p>
    								</td>
									</tr>
								</tbody>
							<%end%>
    				</table>
        </div>
            
    </div>
</div>


    
    <div class="modal fade" id="delete" tabindex="-1" role="dialog" aria-labelledby="edit" aria-hidden="true">
      <div class="modal-dialog">
    <div class="modal-content">
          <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h4 class="modal-title custom_align" id="Heading">Delete this entry</h4>
      </div>
          <div class="modal-body">
       
       <div class="alert alert-warning"><span class="glyphicon glyphicon-warning-sign"></span> Are you sure you want to delete this Record?</div>
       
      </div>
        <div class="modal-footer ">
        <button type="button" class="btn btn-success" ><span class="glyphicon glyphicon-ok-sign"></span> Yes</button>
        <button type="button" class="btn btn-default" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> No</button>
      </div>
        </div>
    <!-- /.modal-content --> 
  </div>
      <!-- /.modal-dialog --> 
    </div>