<div class="container">
    <%@repos.each do |repo_name ,repos|%>
        <div class="row">
            <div class="col-md-6">
              <div class="panel panel-primary">
             
                <div class="panel-heading">
                  <h3 class="panel-title"><%="#{repo_name}"%></h3>
                  <span class="pull-right clickable"><i class="glyphicon glyphicon-chevron-up"></i></span>
                </div>
                <div class="panel-body">
                   <%#binding.pry%>
                    <% repos.each do |repo|%>
                      <div class="col-md-6">
                      <%= link_to "#{repo["github_repo_name"]}",repo_path(repo["id"])%></br>
                      </div>
                    <%end%>
                </div>
              </div>
            </div>
        </div>
    <%end%> 
    <canvas id="canvas" width="900" height="200" ></canvas>  
</div>
<script>
  $(document).ready(function(){
 data = {
  
    labels : <%= "#{@repo_wise_lables}".html_safe%>,
    datasets : [
      /*{
        fillColor : "rgba(220,220,220,0.5)",
        strokeColor : "rgba(220,220,220,1)",
        pointColor : "rgba(220,220,220,1)",
        pointStrokeColor : "#fff",
        data : [65,59,90,81,56,55,40]
      },*/
      {
        fillColor : "rgba(151,187,205,0.5)",
        strokeColor : "rgba(151,187,205,1)",
        pointColor : "rgba(151,187,205,1)",
        pointStrokeColor : "#fff",
        data : <%= "#{@repo_data}"%>
      }
    ]
  }

  myNewChart = new Chart($("#canvas").get(0).getContext("2d")).Line(data)
  });


</script>

  
  
    
    